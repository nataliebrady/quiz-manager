<% provide(:title, @quiz.title) %>
<div class="row mainpage-container">
  <div class="col-md-12">
      <h1>
        <%= @quiz.title %>
        <% @quiz.questions.each do |question| %>
        	<h4><%= question.question_title %></h4>
        	<% question.answers.each do |answer| %>
        		<%= answer.select :answer_title %>
        	<% end %>
        <% end %>
      </h1>
  </div>
</div>